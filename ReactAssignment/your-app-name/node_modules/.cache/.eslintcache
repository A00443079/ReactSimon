<div className=\"textdiv togglebutton absolute\">&lt;&lt;BACK TO HOME</div></a>\n              \n              \n              <a className=\"divbody3sub\">\n                  <br />\n                  <br/>\n                  <img src={Hyderabad} width=\"500px\" />\n                  <h1>I live in Hyderabad, TS, India.</h1>\n              </a>\n              \n                    <a className=\"divbody3sub\">\n                      <Weather />\n                    </a>\n              </div>\n              \n          }\n            </div>\n  );\n}\n\nexport default App;\n","/Users/apple/Desktop/ReactAssignment/ReactAssignment/your-app-name/src/reportWebVitals.js",[],"/Users/apple/Desktop/ReactAssignment/ReactAssignment/your-app-name/src/components/Weather.js",["32","33","34","35","36","37","38","39"],"﻿import React, { Component } from 'react';\nimport Cold from './cold.png';\nimport Mild from './mild.png';\nimport Sunny from './sunny.png';\n\nclass Weather extends Component {\n\n    state = {\n        loading: true,\n        person: null,\n        kelvin: 0,\n        celcius: 0,\n        farenheit: 0,\n        finalvalue: 0,\n        flagcelcius: true,\n        \n        cold: false,\n        mild: false,\n        sunny: false,\n\n    };\n\n    async componentDidMount() {\n        const location = \"Hyderabad\";\n        const url = \"https://api.openweathermap.org/data/2.5/onecall?lat=17.3850&lon=78.4867&exclude=minutely,hourly,daily,alerts&appid=b16f3090c65c3fb6b47c6222d3fbcd60\";\n        const response = await fetch(url);\n        const data = await response.json();\n        this.setState({ person: data.current, loading: false });\n\n        //Set states as COLD, MILD, SUNNY\n        if (this.state.person.temp - 273.15 <= 10) this.setState({ cold: true, mild: false, sunny: false });\n        else if (this.state.person.temp - 273.15 > 10 && this.state.person.temp - 273.15 <= 20) this.setState({ cold: false, mild: true, sunny: false });\n        else this.setState({ cold: false, mild: false, sunny: true });\n\n    }\n\n\n\n\n    \n    render() {\n        return (\n            \n            <div>\n                <h1>WEATHER COMPONENT:</h1>\n                {this.state.loading || !this.state.person ? (<div>loading...</div>)\n                    :\n                    (\n                        <div>\n\n                            {this.state.cold &&\n                                <img src={Cold} />\n                            }\n                            {this.state.mild &&\n                                <img src={Mild} />\n                            }\n                            {this.state.sunny &&\n                                <img src={Sunny} />\n                            }\n\n                            {this.state.flagcelcius && \n                                <div>Temperature in <span>CELCIUS</span>: {this.state.person.temp - 273.15}</div>\n                            }\n\n                            {!this.state.flagcelcius &&\n                                <div>Temperature in <span>FARENHEIT</span>: {(this.state.person.temp - 273.15) * 9 / 5 + 32}</div>\n                            }   \n                            \n                            \n                            \n                            <div>Pressure: {this.state.person.pressure}</div>\n                            <div>Humidity: {this.state.person.humidity}</div>\n                            <div>Sky Condition: {this.state.person.weather[0].description} </div>\n                            \n\n                            <br/>\n                        </div>\n                    )}\n\n                {this.state.flagcelcius &&\n                    <a className=\"textdiv togglebutton\" onClick={() => this.setState({ flagcelcius: !this.state.flagcelcius })}>Change to °F</a>\n                }\n                {!this.state.flagcelcius &&\n                    <a className=\"textdiv togglebutton\" onClick={() => this.setState({ flagcelcius: !this.state.flagcelcius })}>Change to °C</a>\n                }\n                <br /><br /><a className=\"textdiv togglebutton\" onClick={() => this.componentDidMount()}>REFRESH</a>\n            </div>\n            );\n    }\n\n    \n\n    \n\n}\n\nexport default Weather;",{"ruleId":"40","replacedBy":"41"},{"ruleId":"42","replacedBy":"43"},{"ruleId":"44","severity":1,"message":"45","line":2,"column":8,"nodeType":"46","messageId":"47","endLine":2,"endColumn":12},{"ruleId":"48","severity":1,"message":"49","line":20,"column":15,"nodeType":"50","endLine":20,"endColumn":49},{"ruleId":"48","severity":1,"message":"49","line":21,"column":15,"nodeType":"50","endLine":21,"endColumn":49},{"ruleId":"51","severity":1,"message":"52","line":27,"column":15,"nodeType":"50","endLine":27,"endColumn":63},{"ruleId":"48","severity":1,"message":"49","line":33,"column":15,"nodeType":"50","endLine":33,"endColumn":49},{"ruleId":"48","severity":1,"message":"49","line":55,"column":15,"nodeType":"50","endLine":55,"endColumn":49},{"ruleId":"48","severity":1,"message":"49","line":58,"column":15,"nodeType":"50","endLine":58,"endColumn":42},{"ruleId":"51","severity":1,"message":"52","line":61,"column":19,"nodeType":"50","endLine":61,"endColumn":56},{"ruleId":"48","severity":1,"message":"49","line":65,"column":21,"nodeType":"50","endLine":65,"endColumn":48},{"ruleId":"53","severity":1,"message":"54","line":1,"column":1,"nodeType":"55","messageId":"56","fix":"57"},{"ruleId":"44","severity":1,"message":"58","line":24,"column":15,"nodeType":"46","messageId":"47","endLine":24,"endColumn":23},{"ruleId":"51","severity":1,"message":"52","line":52,"column":33,"nodeType":"50","endLine":52,"endColumn":51},{"ruleId":"51","severity":1,"message":"52","line":55,"column":33,"nodeType":"50","endLine":55,"endColumn":51},{"ruleId":"51","severity":1,"message":"52","line":58,"column":33,"nodeType":"50","endLine":58,"endColumn":52},{"ruleId":"48","severity":1,"message":"49","line":81,"column":21,"nodeType":"50","e